                  .form-filter__content
                    .form-filter__content-box
                      label.form-filter__content-label
                        input.filter-style(type="checkbox" checked)
                        span.btn-checked__text SALE
                    .form-filter__content-box
                      label.form-filter__content-label
                        input.filter-style(type="checkbox")
